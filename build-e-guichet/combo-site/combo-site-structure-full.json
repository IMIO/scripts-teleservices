        "subpages_edit_role": null,
        "snapshot": null,
        "groups": []
      },
      "cells": [
        {
          "model": "data.parentcontentcell",
          "fields": {
            "placeholder": "footer",
            "order": 0,
            "slug": "",
            "extra_css_class": "",
            "template_name": null,
